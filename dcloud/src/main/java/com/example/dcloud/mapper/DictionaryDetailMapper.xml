<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dcloud.mapper.DictionaryDetailMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.dcloud.entity.DictionaryDetail">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="type_code" property="typeCode" />
        <result column="name" property="name" />
        <result column="value" property="value" />
        <result column="is_default" property="isDefault" />
        <result column="dict_order" property="dictOrder" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, code, type_code, name, value, is_default, is_delete
    </sql>

</mapper>
