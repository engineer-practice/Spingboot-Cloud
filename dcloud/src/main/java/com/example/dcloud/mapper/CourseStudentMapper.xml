<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dcloud.mapper.CourseStudentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.dcloud.entity.CourseStudent">
        <id column="id" property="id" />
        <result column="course_id" property="courseId" />
        <result column="student_email" property="studentEmail" />
        <result column="teacher_email" property="teacherEmail" />
        <result column="exp" property="exp" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, course_id, student_email, teacher_email
    </sql>

</mapper>
