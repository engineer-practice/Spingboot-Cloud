<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dcloud.mapper.AttendenceResultMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.dcloud.entity.AttendenceResult">
        <id column="id" property="id" />
        <result column="student_email" property="studentEmail" />
        <result column="attend_time" property="attendTime" />
        <result column="is_delete" property="isDelete" />
        <result column="attend_id" property="attendId" />
        <result column="code" property="code" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, student_email, attend_time, is_delete, attend_id, code
    </sql>

</mapper>
