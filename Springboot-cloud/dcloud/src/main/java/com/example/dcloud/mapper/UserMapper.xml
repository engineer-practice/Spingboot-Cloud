<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dcloud.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.dcloud.entity.User">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="nickname" property="nickname" />
        <result column="image" property="image" />
        <result column="sno" property="sno" />
        <result column="sex" property="sex" />
        <result column="telephone" property="telephone" />
        <result column="email" property="email" />
        <result column="password" property="password" />
        <result column="birth" property="birth" />
        <result column="role_id" property="roleId" />
        <result column="exp" property="exp" />
        <result column="state" property="state" />
        <result column="school_code" property="schoolCode" />
        <result column="power_id" property="powerId" />
        <result column="education" property="education" />
        <result column="is_delete" property="isDelete" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, nickname, image, sno, sex, telephone, email, password, birth, role_id, exp, state, school_code, power_id, education, is_delete
    </sql>

</mapper>
